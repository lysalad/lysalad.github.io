	double eng;
	double math;
	int num;
	
	public double StudentInf(double k, double e, double m) {
		kor = k;
		eng = e;
		math = m;
		double sum = k + e + m;
		return sum;
	}
	
	public double StudentInf(double sum, int num) {
		return sum / num;
	}
}

