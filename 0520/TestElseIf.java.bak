/* 컵 사이즈 알려주기 
사용자가 정수를 입력했을 때
200이상이면 large
100이상 200미만이면 medium
100미만이면 small*/

public class TestElseIf{
	public static void main(String [] args) {
		int cup = 150;
		
		String size;
		if (cup >= 200) {
			size = "large";
		}
		
		else if (cup >= 100) {
			size = "medium";
		}
		
		else {
			size = "small";
		}
		
		System.out.println(size);
	}
}